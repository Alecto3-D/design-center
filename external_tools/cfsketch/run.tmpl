body common control
{
      bundlesequence => { "g", "cfsketch_run" };
      inputs => { $inputs };
}

bundle common g
{
  classes:
[% FOREACH var IN booleans.keys.sort %]
      "$var" expression => "[% UNLESS booleans.$var %]![% END %]any";
[% END %]
  vars:
[% FOREACH var IN vars.keys.sort %]
      "$var" [% types.$var %] => [% vars.$var %];
[% END %]
}

bundle agent cfsketch_run
{
  methods:
    ${prefix}__activated::
      "$sketch" usebundle => $entry_point;
}
