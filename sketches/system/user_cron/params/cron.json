            "user": "user",  
            "command": "/bin/echo cron_1 > /dev/null 2>&1",  
        },
        "cron_2":
        { 
            "minute": "0",  
            "hour": "1",  
            "day_of_month": "2",  
            "month": "6",  
            "day_of_week": "*",  
            "user": "user",  
            "command": "/bin/echo cron_2 > /dev/null 2>&1",  
        },

    },
}
